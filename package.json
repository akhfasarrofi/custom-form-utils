{
  "name": "custom-form-utils",
  "version": "0.1.2",
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/src/index.d.ts",
  "keywords": [
    "custom-form-antd",
    "library",
    "react",
    "utils"
  ],
  "author": "dopassss",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akhfasarrofi/custom-form-utils.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --no-cache --max-warnings=0",
    "preview": "vite preview",
    "major": "npm version major",
    "minor": "npm version minor",
    "patch": "npm version patch",
    "postversion": "git push origin main && git push origin --tags && npm publish --access public",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "eslint --fix --no-cache --max-warnings=0",
      "prettier --write"
    ]
  },
  "dependencies": {
    "antd": "^5.13.3",
    "dayjs": "^1.11.10",
    "js-cookie": "^3.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "secure-ls": "^1.2.6"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "8.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.2",
    "vite-tsconfig-paths": "^4.3.1"
  }
}
